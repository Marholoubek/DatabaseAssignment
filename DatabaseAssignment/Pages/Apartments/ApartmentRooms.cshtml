@page
@model StudentAccomodation.Pages.Apartments.ApartmentRoomsModel
@{
}
<h1>Apartments</h1>

<table class="table">
    <thead>
        <tr>
            <th>  Apartment Number   </th>
            <th>    Address  </th>
            <th>  Types  </th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Rooms)
    {
        <tr>
            <td> @item.PlaceNo </td>
            <td> @item.RentPerSemester </td>
            <td> @item.Occupied </td>
            <td> @item.RoomNo </td>
            <td> @item.DormitoryNo </td>
            <td> @item.ApartmentNo </td>
        </tr>}
    </tbody>
</table>